{
  "Servers": {
    "1": {
      "Name": "Docker Postgres",
      "Group": "Servers",
      "Host": "host.docker.internal",
      "Port": 5444,
      "MaintenanceDB": "postgres",
      "Username": "root",
      "SSLMode": "prefer",
      "SSLCert": "<STORAGE_DIR>/.postgresql/postgresql.crt",
      "SSLKey": "<STORAGE_DIR>/.postgresql/postgresql.key",
      "SSLCompression": 0,
      "Timeout": 10,
      "UseSSHTunnel": 0,
      "TunnelPort": "22",
      "TunnelAuthentication": 0
    }
  }
}
